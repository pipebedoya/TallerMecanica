@page
@model Frontend.Pages.MostrarListadoClienteModel//MyApp.Namespace.MostrarListadoClienteModel
@{
}
<div class= "justify-content-center text-center">
    <h1>Listado de Clientes</h1>
    <br>
</div>
<div>
    <br><br>
    <table class= "table">
        <tr>
            <th>ID</th>
            <th>NOMBRE</th>
            <th>APELLIDOS</th>
            <th>CEDULA</th>
            <th>NUMERO TELEFONO</th>

        </tr>
        @foreach(var cliente in Model.Clientes)
        {        
            <tr>
                
                <td>@cliente.Id</td>
                <td>@cliente.Nombre</td>
                <td>@cliente.Apellido</td>
                <td>@cliente.Cedula</td>
            </tr>
        }
    </table>
</div>